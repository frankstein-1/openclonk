<funcs>
  <func>
    <title>LaunchVolcano</title>
    <category>Umwelt</category>
    <version>4.6.5.0 CP <extversion>4.10.0.0 OC</extversion></version>
    <syntax>
      <rtype>bool</rtype>
      <params>
        <param><type>int</type><name>x</name><desc>X-Zielposition</desc></param>
        <param><type>int</type><name>y</name><desc>Y-Zielposition. Default: am unteren Ende der Landschaft</desc><optional/></param>
        <param><type>int</type><name>strength</name><desc>Stärke. Default: relativ zur Höhe der Landschaft</desc><optional/></param>
        <param><type>string</type><name>material_name</name><desc>Name des Materials. Default ist "Lava"</desc><optional/></param>
      </params>
    </syntax>
    <desc>Erzeugt einen Vulkan an angegebener Position</desc>
    <related>
      <funclink>LaunchLightning</funclink> <funclink>LaunchEarthquake</funclink>
    </related>
  </func>
  <author>Newton</author><date>Juli 2009</date>
</funcs>
