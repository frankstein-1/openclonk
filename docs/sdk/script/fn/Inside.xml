<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<!DOCTYPE funcs SYSTEM "../../../clonk.dtd">
<?xml-stylesheet type="text/xsl" href="../../../clonk.xsl"?>
<funcs>
  <func>
    <title>Inside</title>
    <category>Arithmetik</category>
    <version>4.6.5.0 CP</version>
    <syntax>
      <rtype>bool</rtype>
      <params>
        <param>
          <type>int</type>
          <name>value</name>
          <desc>Eingabewert</desc>
        </param>
        <param>
          <type>int</type>
          <name>lower_bound</name>
          <desc>Untergrenze für den Wert</desc>
        </param>
        <param>
          <type>int</type>
          <name>upper_bound</name>
          <desc>Obergrenze für den Wert</desc>
        </param>
      </params>
    </syntax>
    <desc>Gibt TRUE zurück, wenn value &gt;= lower_bound und value &lt;= upper_bound ist.</desc>
    <related>
      <funclink>BoundBy</funclink>
    </related>
  </func>
  <author>PeterW</author><date>November 2001</date>
</funcs>
